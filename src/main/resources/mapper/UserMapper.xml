<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mpdemo.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.mpdemo.entity.User">
        <result column="province" property="userId" />
        <result column="city" property="userName" />
        <result column="zone" property="password" />
        <result column="" property="password"/>
        <result column="desc" property="modifyTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        userId,userName,password,phone,modifyTime
    </sql>

</mapper>
